<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.9.xsd">
    <changeSet id="248c2de4769592dc54f5" author="jobportalcdn">
        <createTable tableName="apply_job">
            <column name="job_id" autoIncrement="true" type="integer">
                <constraints primaryKey="true" primaryKeyName="apply_job_id_pk" unique="true"></constraints>
            </column>
            <column name="first_name" type="varchar(100)">
                <constraints nullable="false"></constraints>
            </column>
            <column name="last_name" type="varchar(100)">
                <constraints nullable="false"></constraints>
            </column>
            <column name="email" type="varchar(100)">
                <constraints nullable="false"></constraints>
            </column>
            <column name="phone" type="varchar(20)">
                <constraints nullable="false"></constraints>
            </column>
            <column name="skills" type="varchar(255)">
                <constraints nullable="false"></constraints>
            </column>
            <column name="resume" type="varchar(100)">
                <constraints nullable="false"></constraints>
            </column>
        </createTable>
    </changeSet>

</databaseChangeLog>